{
  "name" : "Data Rights Extension",
  "private": true,
  "version" : "1.1",
  "description" : "A trivial usage example.",
  "permissions": [
    "browsingData",
    "tabs",
    "background"
  ],
  "browser_action": {
     "default_icon": "icon.png",
     "default_popup": "popup.html"
  },
  "background": {
      "scripts": ["background.js"],
      "persistent": false
  },
  "manifest_version": 2
}
